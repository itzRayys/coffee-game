; Engine configuration file.
; It's best edited using the editor UI and not directly,
; since the parameters that go here are not all obvious.
;
; Format:
;   [section] ; section goes between []
;   param=value ; assign values to parameters

config_version=5

[application]

config/name="coffeeGame"
run/main_scene="res://Assets/Scenes/main.tscn"
config/features=PackedStringArray("4.1", "GL Compatibility")
run/low_processor_mode=true
config/icon="res://icon.svg"

[autoload]

PlayerGlobals="*res://Assets/Scripts/playerGlobals.gd"
AreaSwitcher="*res://Assets/Scripts/areaSwitcher.gd"
DialogueManager="*res://addons/dialogue_manager/dialogue_manager.gd"
GameGlobals="*res://Assets/Scripts/gameGlobals.gd"

[display]

window/size/viewport_width=1920
window/size/viewport_height=1080
window/size/mode=2
window/stretch/mode="viewport"
window/vsync/vsync_mode=0

[dotnet]

project/assembly_name="myFirstGodotGame"

[editor]

movie_writer/disable_vsync=true

[editor_plugins]

enabled=PackedStringArray("res://addons/dialogue_manager/plugin.cfg")

[input]

Jump={
"deadzone": 0.5,
"events": []
}

[input_devices]

pointing/emulate_touch_from_mouse=true

[internationalization]

locale/translations_pot_files=PackedStringArray("res://Assets/Resources/Dialogue/testting.dialogue")

[rendering]

renderer/rendering_method="mobile"
renderer/rendering_method.mobile="gl_compatibility"
environment/defaults/default_clear_color=Color(0.258824, 0.301961, 0.376471, 1)
